<section class="content-header">
</section>
<section class="content">
  <div class="box">
    <%= form_for @shift do |f| %>
      <div class="box-header">
        <h3 class="box-title">New Shift</h3>
        <div class="box-tools pull-right">
          <div class="btn-group">
            <%= link_to "Cancel",shifts_path, class: "btn btn-danger" %>
          </div>

          <%= f.submit "Create", class: "btn btn-primary" %>
        </div>
      </div>
      <div class="box-body">``

        <div class="form-group">
          <%= f.label :start_date %>
          <%= f.text_field :start_date, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :end_date %>
          <%= f.text_field :end_date, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :user_id %>
          <%= f.text_field :user_id, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :starting_cash %>
          <%= f.text_field :starting_cash, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :payments %>
          <%= f.text_field :payments, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :paid_in %>
          <%= f.text_field :paid_in, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :paid_out %>
          <%= f.text_field :paid_out, class: "form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :cash %>
          <%= f.text_field :cash, class: "form-control"%>
        </div>

      </div>
    <% end %>
  </div>
</section>
